{
	"name": "daggercuelsp",
	"displayName": "dagger cue lsp",
	"description": "A VSCode extension to use dagger cue lsp",
	"icon": "dagger-icon.png",
	"publisher": "Dagger",
	"repository": {
		"url": "https://github.com/dagger/vscode-dagger"
	},
	"version": "0.3.0",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Language Packs",
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:daggerlsp.start",
		"onLanguage:cue"
	],
	"main": "./out/extension.js",
	"contributes": {
		"themes": [
			{
				"uiTheme": "vs-dark",
				"label": "Dagger Theme",
				"path": "./themes/dagger/theme.json"
			}
		],
		"languages": [
			{
				"id": "cue",
				"aliases": [
					"Cue",
					"cue"
				],
				"extensions": [
					".cue"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"commands": [
			{
				"command": "daggerlsp.start",
				"title": "DAGGERLSP: Start"
			},
			{
				"command": "dlsp.restart",
				"title": "DAGGERLSP: Restart"
			},
			{
				"command": "dlsp.showLogs",
				"title": "DAGGERLSP: Show Logs"
			}
		]
	},
	"configuration": {
		"title": "Trace level",
		"properties": {
			"dlsp.trace.server": {
				"type": "string",
				"enum": [
					"off",
					"messages",
					"verbose"
				],
				"default": "verbose",
				"description": "Level of verbosity in dlsp's log output.",
				"scope": "window"
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^10.0.0",
		"typescript": "^4.3.2",
		"@vscode/test-electron": "^1.5.2"
	},
	"configurationDefaults": {
		"cue": {
			"editor.semanticHighlighting.enabled": true
		}
	},
	"dependencies": {
		"vscode-languageclient": "^8.0.0"
	}
}